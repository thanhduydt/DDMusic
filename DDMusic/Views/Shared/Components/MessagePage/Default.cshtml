@model DDMusic.Views.Shared.Components.MessagePage.MessagePage.Message
@{
    Layout = "_Layout";
    ViewData["Title"] = @Model.title;
}
<section class="login-area section-padding-200">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-8">
                <div class="login-content">
                    <div class="login-form">
                        <center>
                            <h4>@Model.title</h4>
                            @Html.Raw(Model.htmlcontent)
                        </center>
                                            
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>